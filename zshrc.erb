ZSH_THEME="wezm"
plugins=(git common-aliases rails bundler gem npm yarn)

source $ZSH/oh-my-zsh.sh
source <%= ENV['HOME'] %>/.aliases
source <%= ENV['HOME'] %>/.functions

bindkey '[C' forward-word # [Ctrl-RightArrow] - move forward one word
bindkey '[D' backward-word # [Ctrl-LeftArrow] - move backward one word

if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi

eval "$(pyenv init -)"
eval "$(pyenv virtualenv-init -)"
pyenv shell 3.8.0

cd ~/Projects
